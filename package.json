{
  "name": "langri-sha.com",
  "description": "Langri-Sha org",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --progress --open --hot --https --port 8080",
    "build": "webpack --env production --progress",
    "test": "webpack --debug --bail",
    "flow-typed-mono": "yarn workspaces run flow-typed --flowVersion $(cat .flowconfig | head -n2 | tail -n1) install --rootDir $(pwd) --skip",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langri-sha/langri-sha.com"
  },
  "keywords": [
    "gh-pages",
    "webpack",
    "react"
  ],
  "author": "Filip DupanoviÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langri-sha/langri-sha.github.com/issues"
  },
  "homepage": "https://github.com/langri-sha/langri-sha.github.com#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "7.16.10",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "cssnano": "^5.0.15",
    "normalize.css": "8.0.1",
    "preact": "10.6.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "source-map-support": "0.5.21",
    "webfontloader": "1.6.28",
    "webpack": "5.66.0",
    "webpack-cli": "4.9.1"
  },
  "devDependencies": {
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.7.0",
    "flow-bin": "0.169.0",
    "flow-typed": "3.6.1",
    "husky": "7.0.4",
    "lint-staged": "12.2.2",
    "prettier": "2.5.1",
    "prettier-cli": "0.1.0",
    "stylelint": "14.2.0",
    "stylelint-config-css-modules": "2.3.0",
    "stylelint-config-standard": "24.0.0"
  }
}
