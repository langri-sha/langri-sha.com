{
  "name": "langri-sha.github.io",
  "version": "1.0.0-alpha",
  "description": "GitHub Pages site for Langri-Sha",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --open --hot --watch --https --port 8080",
    "build": "webpack --env.production --progress",
    "test": "webpack --debug --bail",
    "lint": "xo"
  },
  "engines": {
    "node": ">=10.14.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langri-sha/langri-sha.github.io.git"
  },
  "keywords": [
    "gh-pages",
    "webpack",
    "react"
  ],
  "author": "Filip DupanoviÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langri-sha/langri-sha.github.io/issues"
  },
  "homepage": "https://github.com/langri-sha/langri-sha.github.io#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": true,
    "singleQuote": true
  },
  "xo": {
    "extends": [
      "xo/browser",
      "xo-react"
    ],
    "env": [
      "browser",
      "node"
    ],
    "globals": ["ga"],
    "parser": "babel-eslint",
    "prettier": true,
    "semicolon": false,
    "space": true
  },
  "lint-staged": {
    "*.{js,json}": [
      "xo --fix",
      "git add"
    ],
    "*.{js}": [
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@emotion/core": "10.0.5",
    "@emotion/styled": "10.0.5",
    "classnames": "2.2.6",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "1.0.0",
    "commitizen": "3.0.5",
    "copy-webpack-plugin": "4.6.0",
    "cssnano": "^4.1.8",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.11.1",
    "eslint-config-xo": "0.25.1",
    "eslint-config-xo-flow": "0.1.0",
    "eslint-config-xo-react": "0.17.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-react": "7.12.0",
    "file-loader": "3.0.1",
    "flow-bin": "0.89.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "normalize.css": "8.0.1",
    "preact": "8.4.2",
    "prettier": "1.15.3",
    "prettier-cli": "0.1.0",
    "raw-loader": "1.0.0",
    "source-map-support": "0.5.9",
    "stylelint": "9.9.0",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-standard": "18.2.0",
    "terser-webpack-plugin": "1.2.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.2",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "xo": "0.23.0"
  }
}
