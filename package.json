{
  "name": "langri-sha.com",
  "description": "Langri-Sha org",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --progress --open --hot --https --port 8080",
    "build": "webpack --env production --progress",
    "test": "webpack --debug --bail",
    "flow-typed-mono": "yarn workspaces run flow-typed --flowVersion $(cat .flowconfig | head -n2 | tail -n1) install --rootDir $(pwd) --skip",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langri-sha/langri-sha.com"
  },
  "keywords": [
    "gh-pages",
    "webpack",
    "react"
  ],
  "author": "Filip DupanoviÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langri-sha/langri-sha.github.com/issues"
  },
  "homepage": "https://github.com/langri-sha/langri-sha.github.com#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "7.14.6",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "webfontloader": "1.6.28"
  },
  "devDependencies": {
    "@langri-sha/webpack": "1.0.0-next",
    "commitizen": "3.0.5",
    "cssnano": "^4.1.8",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "7.30.0",
    "flow-bin": "0.155.0",
    "flow-typed": "3.3.1",
    "husky": "7.0.1",
    "lint-staged": "11.0.0",
    "normalize.css": "8.0.1",
    "preact": "8.4.2",
    "prettier": "2.3.2",
    "prettier-cli": "0.1.0",
    "source-map-support": "0.5.9",
    "stylelint": "9.9.0",
    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-standard": "18.2.0",
    "webpack": "5.44.0",
    "webpack-cli": "4.7.2"
  }
}
